public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String [] genre=new String[5];
		genre[0]="Amour";
		@SuppressWarnings("deprecation")
		Date date=new Date(23,2,2015);
		
		Movie mov=new Movie("Chabada",date,15);
		Room ro =new Room(10,10,mov);
		Seat s1=new Seat(9,4);
		Seat s2=new Seat(9,3);
		Customer c1=new Customer(s1,ro);
		Customer c2=new Customer(s2,ro);
		Customer c3=new Customer(s2,ro);
		for(int i=0;i<ro.getRows;i++){
			for(j=0;j<ro.getSeatsByRow();j++){
				ro.getAllSeats().get(i)..get(j).isTaken();
			}
		}

		
	}

}
